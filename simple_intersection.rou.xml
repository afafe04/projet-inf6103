<?xml version="1.0" encoding="UTF-8"?>
<routes>
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" maxSpeed="15"/>
    
    <!-- North to South (straight) -->
    <route id="north_south" edges="n_to_c c_to_s"/>
    <!-- East to West (straight) -->
    <route id="east_west" edges="e_to_c c_to_w"/>
    <!-- South to North (straight) -->
    <route id="south_north" edges="s_to_c c_to_n"/>
    <!-- West to East (straight) -->
    <route id="west_east" edges="w_to_c c_to_e"/>
    
    <!-- Right turns -->
    <route id="north_east" edges="n_to_c c_to_e"/>
    <route id="east_south" edges="e_to_c c_to_s"/>
    <route id="south_west" edges="s_to_c c_to_w"/>
    <route id="west_north" edges="w_to_c c_to_n"/>
    
    <!-- Left turns -->
    <route id="north_west" edges="n_to_c c_to_w"/>
    <route id="east_north" edges="e_to_c c_to_n"/>
    <route id="south_east" edges="s_to_c c_to_e"/>
    <route id="west_south" edges="w_to_c c_to_s"/>
    
    <!-- Add vehicles with different colors and times -->
